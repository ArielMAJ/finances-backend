{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "mvn clean package",
  "outputDirectory": "target",
  "functions": {
    "api/**/*.java": {
      "runtime": "vercel-java@1.0.0",
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.java"
    }
  ]
}
